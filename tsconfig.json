{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015", "dom"],
    "module": "ESNext",
    "strict": true,
    "typeRoots": [],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "sourceMap": true,
  },
  "include": ["src/**/*", "images.d.ts"],
  "exclude": ["node_modules"]
}
